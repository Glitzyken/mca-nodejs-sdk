{
  "name": "mca-nodejs-sdk",
  "version": "1.0.0",
  "description": "Unofficial Node.js SDK tailored for MyCover.ai. Seamlessly integrate and supercharge your insurance offerings with Africa's foremost insure-tech infrastructure.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "build": "tsup",
    "dev": "ts-node example",
    "pub:local": "npm run build && npm pack --pack-destination ~/Desktop",
    "prepare": "husky install"
  },
  "keywords": [
    "mycoverai",
    "insurance",
    "sdk"
  ],
  "author": "Kenneth Jimmy",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.198",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
